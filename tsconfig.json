{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@layouts/*": ["src/layouts/*"],
            "@models/*": ["src/models/*"],
            "@pages/*": ["src/pages/*"],
            "@services/*": ["src/services/*"],
            "@styles/*": ["src/styles/*"]
        }
    }
}
